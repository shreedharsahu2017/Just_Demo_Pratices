package com.jsp.String;

public class Revese3 {

	public static void main(String[] args) {
		System.out.println(reverse("java"));
	}
	  static String reverse(String a) {
	 char[]	c=a.toCharArray();
	 int i=0,j=a.length()-1;
	 while(i<j) {
		 char temp=c[i];
		 c[i]=c[j];
		 c[j]=temp;
		 i++;
		 j--;
	 }
	 String s=new String(c);
	  return s;
	}

}
