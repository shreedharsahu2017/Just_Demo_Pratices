package com.jsp.String;

public class Splitdemo {

	public static void main(String[] args) {
		String s1="java is easy";
		String[] a=s1.split("");
		/*for(String s:a)
			System.out.print(s);*/
		String s2="";
		for(int i=a.length-1;i>=0;i--) {
			s2=s2+a[i];
			if(i!=0)
				s2=s2+"";
		}
		System.out.println(s2);
	}

}
